#include<Servo.h>
  Servo s1, s2, s3, s4, s5, s6, s7, s8, s9, s10, s11, s12, s13, s14, s15, s16, s17, s18;


void setup() {

  s1.attach(22);
  s1.writeMicroseconds(1500);

  s2.attach(23);
  s2.writeMicroseconds(1500);

  s3.attach(24);
  s3.writeMicroseconds(1500);
s4.attach(26);
  s4.writeMicroseconds(1500);
